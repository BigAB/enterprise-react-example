{
  "version": 1,
  "projects": {
    "initiative-management": {
      "root": "apps/initiative-management",
      "sourceRoot": "apps/initiative-management/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/initiative-management",
            "index": "apps/initiative-management/src/index.html",
            "main": "apps/initiative-management/src/main.tsx",
            "polyfills": "apps/initiative-management/src/polyfills.ts",
            "tsConfig": "apps/initiative-management/tsconfig.app.json",
            "assets": [
              "apps/initiative-management/src/favicon.ico",
              "apps/initiative-management/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/initiative-management/src/environments/environment.ts",
                  "with": "apps/initiative-management/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "initiative-management:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "initiative-management:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/initiative-management/.eslintrc",
            "tsConfig": [
              "apps/initiative-management/tsconfig.app.json",
              "apps/initiative-management/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/initiative-management/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/initiative-management/jest.config.js",
            "tsConfig": "apps/initiative-management/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "initiative-management-e2e": {
      "root": "apps/initiative-management-e2e",
      "sourceRoot": "apps/initiative-management-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/initiative-management-e2e/cypress.json",
            "tsConfig": "apps/initiative-management-e2e/tsconfig.e2e.json",
            "devServerTarget": "initiative-management:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "initiative-management:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/initiative-management-e2e/.eslintrc",
            "tsConfig": ["apps/initiative-management-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/initiative-management-e2e/**"
            ]
          }
        }
      }
    },
    "design-system": {
      "root": "libs/design-system",
      "sourceRoot": "libs/design-system/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/design-system/.eslintrc",
            "tsConfig": [
              "libs/design-system/tsconfig.lib.json",
              "libs/design-system/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/design-system/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/design-system/jest.config.js",
            "tsConfig": "libs/design-system/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/design-system/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/design-system",
            "config": {
              "configFolder": "libs/design-system/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "design-system-e2e": {
      "root": "apps/design-system-e2e",
      "sourceRoot": "apps/design-system-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/design-system-e2e/cypress.json",
            "tsConfig": "apps/design-system-e2e/tsconfig.e2e.json",
            "devServerTarget": "design-system:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "design-system:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/design-system-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/design-system-e2e/**"]
          }
        }
      }
    },
    "domain-types": {
      "root": "libs/domain-types",
      "sourceRoot": "libs/domain-types/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/domain-types/tsconfig.lib.json",
              "libs/domain-types/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/domain-types/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/domain-types/jest.config.js",
            "tsConfig": "libs/domain-types/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "initiative-management"
}
